{
  "name": "contact_list_playwright_typescript_automation",
  "version": "1.0.0",
  "description": "This is an automation using Playwright Typescript on Contact List Website",
  "main": "index.js",
  "scripts": {
    "test": "playwright test",
    "report": "playwright show-report",
    "test:chromium": "playwright test --project=chromium",
    "test:chromium:tag": "playwright test --project=chromium -g",
    "test:firefox": "playwright test --project=firefox",
    "test:firefox:tag": "playwright test --project=firefox -g",
    "test:edge": "playwright test --project=edge",
    "test:edge:tag": "playwright test --project=edge -g",
    "test:webkit": "playwright test --project=webkit",
    "test:webkit:tag": "playwright test --project=webkit -g",
    "test:browser-all:tag": "playwright test -g",
    "test:ui:browser": "playwright test --ui",
    "generate:allure": "allure generate reports/allure-results --single-file --output reports/allure-reports --clean"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@faker-js/faker": "^9.8.0",
    "@playwright/test": "^1.52.0",
    "@types/axios": "^0.9.36",
    "@types/date-fns": "^2.5.3",
    "@types/dotenv": "^6.1.1",
    "@types/loglevel": "^1.5.4",
    "@types/node": "^24.0.3",
    "allure-js-commons": "^3.2.1",
    "allure-playwright": "^3.2.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.5.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "axios": "^1.10.0",
    "date-fns": "^4.1.0",
    "loglevel": "^1.9.2"
  }
}
